<h1>Categories</h1>
<%= link_to "+ New Category", new_admin_category_path %>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
    <tr>
      <td><%= category.name %></td>
      <td>
        <%= link_to "View", admin_category_path(category) %>
        <%= link_to "Edit", edit_admin_category_path(category) %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>